{% extends 'base.html.twig' %}
{% block body -%}
    <div class="container-fluid main">
        <div class="row">
            <div class="col">
                <div class="page-header">
                    <h1>Gang</h1>
                </div>
                <table class="record_properties">
                    <tbody>
                    <tr>
                        <th>Id</th>
                        <td>{{ entity.id }}</td>
                    </tr>
                    <tr>
                        <th>Code</th>
                        <td>{{ entity.code }}</td>
                    </tr>
                    <tr>
                        <th>Name</th>
                        <td>{{ entity.name }}</td>
                    </tr>
                    </tbody>
                </table>
                <ul class="record_actions">
                    <li>
                        <a href="{{ path('admin_gang') }}">
                            Back to the list
                        </a>
                    </li>
                    <li>
                        <a href="{{ path('admin_gang_edit', { 'id': entity.id }) }}">
                            Edit
                        </a>
                    </li>
                    <li>{{ form(delete_form) }}</li>
                </ul>
            </div>
        </div>
    </div>
{% endblock %}
